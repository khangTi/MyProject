<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.kt.myproject.ui.fragment.home.HomeFragment"
        android:label="home"
        tools:layout="@layout/home" />
    <action
        android:id="@+id/action_global_homeFragment"
        app:destination="@id/homeFragment" />

    <dialog
        android:id="@+id/cartDialog"
        android:name="com.kt.myproject.ui.dialog.CartDialog"
        android:label="CartDialog"
        tools:layout="@layout/cart_dialog"/>
    <action
        android:id="@+id/action_global_cartDialog"
        app:destination="@id/cartDialog" />

    <fragment
        android:id="@+id/repeatFragment"
        android:name="com.kt.myproject.ui.fragment.repeat.RepeatFragment"
        android:label="basic_coroutine"
